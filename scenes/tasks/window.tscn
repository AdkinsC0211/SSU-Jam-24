[sub_resource type="GDScript" id="GDScript_0dbjc"]
script/source = "extends InteractableStaticBody3D

#blinds variables
var blinds_open: bool = true
var blinds_offset_x: float = -3.5

@onready var window_parent = self.get_parent()

# Called when the node enters the scene tree for the first time.
func _ready():
	self.interactMessage = \"Press E to close blinds\"


# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(_delta):
	pass


func interact(_body: CharacterBody3D) -> void:
	if window_parent.window_open == false:
		#self.translate(Vector3(blinds_offset_x,0,0))
		blinds_offset_x *= -1
		if blinds_open: 
			$AnimationPlayer.play(\"open_blinds\",-1,1,false)
			self.interactMessage = \"Press E to open blinds\"
		else:
			$AnimationPlayer.play_backwards(\"open_blinds\",-1)
			self.interactMessage = \"Press E to close blinds\"
		blinds_open = !blinds_open
	
"

